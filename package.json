{
  "name": "kuchidomeseyo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:build": "prisma generate --no-engine && TEST_BUILD=true next build",
    "build": "prisma generate --no-engine && next build && npm run sitemap",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write '**/*.(ts|tsx)'",
    "sitemap": "next-sitemap --config sitemap.config.js",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:preview": "npm run pages:build && wrangler pages dev .vercel/output/static --compatibility-date=2024-02-08 --compatibility-flag=nodejs_compat",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@prisma/extension-accelerate": "^0.6.3",
    "lru-cache": "^10.2.0",
    "next": "14.1.0",
    "next-sitemap": "^4.2.3",
    "prisma": "^5.9.1",
    "qrcode.react": "^3.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.6",
    "@cloudflare/workers-types": "^4.20240208.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "^1.8.6",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "^33.5.0",
    "wrangler": "^3.28.1"
  }
}
